#tag Events flastbutton
	#tag Event
		Sub Action()
		  dim i,z as integer
		  dim s as string
		  
		  s =""
		  z = countFields(fnfield.text," ")
		  if z < 2 then
		    snfield.text = fnfield.text
		  else
		    for i = 1 to z - 1
		      s = s + left(nthfield(fnfield.text," ",i),1) + " "
		    next
		    s = s + nthfield(fnfield.text," ",z)
		    snfield.text = s
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events firstlbutton
	#tag Event
		Sub Action()
		  dim i,z as integer
		  dim s as string
		  
		  s =""
		  z = countFields(fnfield.text," ")
		  if z < 2 then
		    snfield.text = fnfield.text
		  else
		    s = nthField(fnfield.text," ",1)
		    for i = 2 to z
		      s = s + " " + left(nthfield(fnfield.text," ",i),1)
		    next
		    snfield.text = s
		  end if
		End Sub
	#tag EndEvent

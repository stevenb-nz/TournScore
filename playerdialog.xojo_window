#tag Events okaybutton
	#tag Event
		Sub Action()
		  dim p as Player
		  
		  if playerid > -1 then
		    app.playerlist(playerid).fullname = fnfield.text
		    app.playerlist(playerid).abbrname = snfield.text
		    app.playerlist(playerid).club = clubfield.text
		    app.playerlist(playerid).realrating = val(ratingfield.text)
		    if sortratingfield.text = "" then
		      app.playerlist(playerid).sortrating = -1
		    else
		      app.playerlist(playerid).sortrating = val(sortratingfield.text)
		    end if
		  else
		    p = new Player
		    p.fullname = fnfield.text
		    p.abbrname = snfield.text
		    p.club = clubfield.text
		    p.realrating = val(ratingfield.text)
		    if sortratingfield.text = "" then
		      p.sortrating = -1
		    else
		      p.sortrating = val(sortratingfield.text)
		    end if
		    app.playerlist.append p
		  end if
		  close
		  app.resortplayers
		End Sub
	#tag EndEvent
#tag EndEvents

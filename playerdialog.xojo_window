#tag WindowCode
	#tag Method, Flags = &h0
		Sub playerokaycheck()
		  dim playerokaycheckb as Boolean
		  dim i as integer
		  
		  playerokaycheckb = true
		  if fnfield.Text = "" then
		    playerokaycheckb = false
		  end if
		  if snfield.Text = "" then
		    playerokaycheckb = false
		  else
		    for i = 0 to UBound(app.playerlist)
		      if snfield.text = app.playerlist(i).abbrname then
		        if i <> Playerid then
		          playerokaycheckb = false
		        end if
		      end if
		    next
		  end if
		  if clubfield.text = "" then
		    playerokaycheckb = false
		  end if
		  okaybutton.Enabled = playerokaycheckb
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		playerid As Integer
	#tag EndProperty


#tag EndWindowCode


	#tag Event
		Sub LostFocus()
		  dim i as integer
		  dim s as string
		  
		  s = ""
		  for i = 1 to len(me.text)
		    if mid(me.text,i,1) <> " " then
		      s = s + mid(me.text,i,1)
		    end if
		  next
		  me.text = uppercase(left(s,3))
		End Sub
	#tag EndEvent
	#tag Event
		Sub TextChange()
		  playerokaycheck
		End Sub

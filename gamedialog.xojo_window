#tag Events swapbutton
	#tag Event
		Sub Action()
		  dim j as integer
		  dim temp as string
		  
		  if me.caption="Swap All" then
		    temp = p1.text
		    p1.text = p2.text
		    p2.text = temp
		    temp = p1score.text
		    p1score.text = p2score.text
		    p2score.text = temp
		    temp = p1bonus.text
		    p1bonus.text = p2bonus.text
		    p2bonus.text = temp
		    for j=0 to 2
		      if P1Attend(j).Value then
		        temp = P1Attend(j).Caption
		      end if
		    next
		    for j=0 to 2
		      if P2Attend(j).Value then
		        P1Attend(j).Value = true
		      end if
		    next
		    for j=0 to 2
		      if P2Attend(j).Caption = temp then
		        P2Attend(j).Value = true
		      end if
		    next
		  else
		    temp = p1.text
		    p1.text = p2.text
		    p2.text = temp
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents

#tag Events P1Attend
	#tag Event
		Sub Action(index as Integer)
		  if p1attend(0).Value then
		    if P2Attend(0).Value then
		      p1result.Text = ""
		      p1result.Visible = false
		      p2result.Text = ""
		      p2result.Visible = false
		      p1score.Visible = true
		      p1bonus.Visible = true
		      p2score.Visible = true
		      p2bonus.Visible = true
		      spreadfield.Visible = true
		    else
		      p1score.Visible = false
		      p1bonus.Visible = false
		      p2score.Visible = false
		      p2bonus.Visible = false
		      spreadfield.Visible = false
		      p1result.Text = "WIN"
		      p1result.Visible = true
		    end if
		  elseif p1attend(1).Value then
		    p1score.Visible = false
		    p1bonus.Visible = false
		    p2score.Visible = false
		    p2bonus.Visible = false
		    spreadfield.Visible = false
		    p1result.Text = ""
		    p1result.Visible = true
		    if P2Attend(0).Value then
		      p2result.Text = "WIN"
		      p2result.Visible = true
		    end if
		  else
		    p1score.Visible = false
		    p1bonus.Visible = false
		    p2score.Visible = false
		    p2bonus.Visible = false
		    spreadfield.Visible = false
		    p1result.Text = "LOSS"
		    p1result.Visible = true
		    if P2Attend(0).Value then
		      p2result.Text = "WIN"
		      p2result.Visible = true
		    end if
		  end if
		  gameokaycheck
		  
		End Sub
	#tag EndEvent
#tag EndEvents

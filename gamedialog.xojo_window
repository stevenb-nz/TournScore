#tag Events clearButton
	#tag Event
		Sub Action()
		  app.gamelist(gameindex).p1 = p1.text
		  app.gamelist(gameindex).p2 = p2.text
		  app.gamelist(gameindex).p1score = 0
		  app.gamelist(gameindex).p2score = 0
		  app.gamelist(gameindex).p1bonus = 0
		  app.gamelist(gameindex).p2bonus = 0
		  app.gamelist(gameindex).p1attended = "Present"
		  app.gamelist(gameindex).p2attended = "Present"
		  app.gamelist(gameindex).played = false
		  currentroundwindow.refresh
		  close
		End Sub

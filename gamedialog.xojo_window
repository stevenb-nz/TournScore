#tag Events okbutton
	#tag Event
		Sub Action()
		  dim j as integer
		  
		  if app.gamelist(gameindex).p1 = p1.text then
		    app.gamelist(gameindex).p1score = val(p1score.text)
		    app.gamelist(gameindex).p2score = val(p2score.text)
		    app.gamelist(gameindex).p1bonus = val(p1bonus.text)
		    app.gamelist(gameindex).p2bonus = val(p2bonus.text)
		    for j=0 to 2
		      if P1Attend(j).Value then
		        app.gamelist(gameindex).p1attended = P1Attend(j).Caption
		      end if
		    next
		    for j=0 to 2
		      if P2Attend(j).Value then
		        app.gamelist(gameindex).p2attended = P2Attend(j).Caption
		      end if
		    next
		  else
		    app.gamelist(gameindex).p1score = val(p2score.text)
		    app.gamelist(gameindex).p2score = val(p1score.text)
		    app.gamelist(gameindex).p1bonus = val(p2bonus.text)
		    app.gamelist(gameindex).p2bonus = val(p1bonus.text)
		    for j=0 to 2
		      if P2Attend(j).Value then
		        app.gamelist(gameindex).p1attended = P2Attend(j).Caption
		      end if
		    next
		    for j=0 to 2
		      if P1Attend(j).Value then
		        app.gamelist(gameindex).p2attended = P1Attend(j).Caption
		      end if
		    next
		  end if
		  app.gamelist(gameindex).played = true
		  currentroundwindow.refresh
		  close
		End Sub

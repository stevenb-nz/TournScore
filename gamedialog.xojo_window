	#tag Method, Flags = &h0
		Sub gameokaycheck()
		  if P1Attend(0).Value and P2Attend(0).Value then
		    if p1score.text = "" or p2score.text = "" or abs(val(p1score.text) - val(p2score.text)) <> val(spreadfield.text) then
		      okbutton.enabled = false
		    else
		      okbutton.enabled = true
		    end if
		  else
		    okbutton.enabled = true
		  end if
		End Sub
	#tag EndMethod



#tag Events p1
	#tag Event
		Function ConstructContextualMenu(base as MenuItem, x as Integer, y as Integer) As Boolean
		  dim g,i as integer
		  dim m as MenuItem
		  
		  for i=0 to UBound(app.playerlist)
		    if app.playerlist(i).abbrname = p1.text then
		      g = app.playerlist(i).grade
		    end if
		  next
		  for i=0 to UBound(app.playerlist)
		    if app.playerlist(i).grade = g then
		      m = new menuitem
		      m.text = app.playerlist(i).abbrname
		      base.Append m
		    end if
		  next
		  return true
		End Function
	#tag EndEvent
	#tag Event
		Function ContextualMenuAction(hitItem as MenuItem) As Boolean
		  p1.text = hitItem.text
		  return true
		End Function
	#tag EndEvent
#tag EndEvents
